{
	"name": "cmsgears/module-core",
	"description": "The core module for all the projects based on CMSGears.",
	"homepage": "http://www.cmsgears.org", 
	"type": "package",
	"keywords": [ "yii2", "cmsgears", "module", "core" ],
	"license": "GPLv3",
	"authors": [
		{
			"name": "Bhagwat Singh Chouhan",
			"email": "bhagwat.chouhan@vulpinecode.com",
			"homepage": "http://www.cmsgears.org/",
			"role": "Founder and project lead."
		}
	],
	"support": {
        "source": "http://www.cmsgears.org/downloads/modules/core",
		"forum": "http://forum.cmsgears.org/",
		"resources": "http://resources.cmsgears.org/",
		"issues": "http://cmsgears.amazingdesk.com/issues/",
		"blog": "http://cmsgears.tutorials24x7.com/"
	},
	"autoload": {
		"psr-4": {
			"cmsgears\\core\\": ""
		}
	},
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "2.0.3",
        "yiisoft/yii2-swiftmailer": "2.0.3"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}